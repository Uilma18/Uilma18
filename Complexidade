import java.util.Random;

public class Complexidade {
    public static void main(String[] args){
        Random random = new Random();
        int [][] mat = new int[4] [4];
        int num = 4;

        for(int i = 0; i < num; i++){
           for(int j = 0; j < num; j++){
              if(num == mat[i] [j]) {

                  System.out.println( "Achou!");
              }
           }
        }
    }
}
